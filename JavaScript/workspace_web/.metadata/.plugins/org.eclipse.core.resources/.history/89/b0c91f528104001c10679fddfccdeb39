<script>
	//객체 : 100, 김사원, 45600.99 출력가능함수
	var emp1 = {
			id:100,
			name:'김사원',
			salary:45600.99,
			print:function(){
				documnet.write("<h3>"+id+":"+name+":"+salary+"</h3>");
			}
	}
	
	emp1.print(); //객체 내부의 함수 호출 window객체는 예외임 걔넨 함수명만 나와도 됨
	
</script>
