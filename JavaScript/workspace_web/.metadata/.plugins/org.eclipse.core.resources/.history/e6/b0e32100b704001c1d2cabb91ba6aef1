<script type="text/javascript">
function f1(){
	alert('이벤트발생');
}
//고전이벤트모델
window.onload = function(){
var btn2 =documet.getElementById("btn2");
btn2.onclick = function(){
	alert("버튼2 이벤트 발생");
}
}
</script>
</head>
<body>
<form action="a.html">
<input type=text><br>
<!-- inline 이벤트 모델 html태그 종류 -->
