	var f2 = function (i){
		return i+i;
	}
	
	document.write(typeof f2 + "<br>");
	
	var r2 = f2(1);
	var r3 = f2(3.14);
	document.write(r2 + "<br>");
	document.write(r3 + "<br>");
	document.write(f2("script") + "<br>");
	
	var f3 = function () {
		documenmt.write("무명함수 실행 <br>");
	}
	document.write(f3() + "<br>");
</script>
